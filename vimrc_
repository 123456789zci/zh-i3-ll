function Gdb()
exec "w"
exec "!gcc -g % -o %<"
endfunction

function GDB() 
exec "w"
set splitbelow
:term gdb %<
endfunction

function  CompileRunGcc()
exec "w"
if &filetype == 'c'
		exec "!gcc % -o %<"
	elseif &filetype == 'cpp'
		exec "!g++ % -o %<"
	elseif &filetype == 'java'
		exec "!javac %"
		exec "!time java %<"
	elseif &filetype == 'sh'
		:!time bash %
	elseif &filetype == 'python'
		exec "!time python2.7 %"
    elseif &filetype == 'html'
        exec "!firefox % &"
    elseif &filetype == 'go'
        exec "!go build %<"
        exec "!time go run %"
    elseif &filetype == 'mkd'
        exec "!~/.vim/markdown.pl % > %.html &"
        exec "!firefox %.html &"
	endif
	
endfunction

function  CompileRungcc()
exec "w"
set splitbelow
:term ./%< 
endfunction


